<Page x:Class="Proyecto_Comunidad.Page2"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Proyecto_Comunidad"
      mc:Ignorable="d" 
      d:DesignHeight="450"
      Title="Page2" Width="270" Height="421">
    <GroupBox Header="Entrance Data" BorderBrush="#FFF248B1" BorderThickness="3,3,3,3" Margin="0,0,10,10">
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Button x:Name="generarVecinos" Content="GENERATE OWNERS" HorizontalAlignment="Center" Width="212" Margin="0,334,0,12" FontWeight="Bold" Click="generarVecinos_Click"/>
            <Label x:Name="NUMENTRANCES" Content="Generate Owners" HorizontalAlignment="Center" Margin="0,25,0,0" VerticalAlignment="Top" FontWeight="Bold" FontSize="20"/>
            <Button x:Name="distribuirVecinos" Content="DISTRIBUTE OWNERS" HorizontalAlignment="Center" Width="212" Margin="0,258,0,88" FontWeight="Bold" Click="distribuirVecinos_Click" IsEnabled="False"/>
        </Grid>
    </GroupBox>
</Page>
